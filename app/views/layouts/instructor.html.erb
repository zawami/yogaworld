<!DOCTYPE html>
<html>
  <head>
    <title><%= document_title %></title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%=stylesheet_link_tag "instructor",media:"all","data-turbolinks-track":true %>
    <%# <%=stylesheet_pack_tag "instructor",media:"all","data-turbolinks-track":true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag "application", "data-turbolinks-track": "reload" %>
  </head>
  <body>
    <div id="wrapper">
      <%= render "instructor/shared/header" %>
      <div id="container">
        <% flash.each do |message_type, message| %>
        <div class="alert alert-<%= message_type %>"><%= message %></div>
      <% end %>
        <%= yield %>
      </div>
      <%= render "shared/footer" %>
    </div>
    <script>
      $(function(){
        $(".alert").fadeOut(5000);
      });
    </script>
    <%= debug(params) if Rails.env.development? %>
    <%= debug @comments  if Rails.env.development? %>
  </body>
</html>


